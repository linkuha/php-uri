  "keywords": [
    "url",
    "uri",
    "rfc3986",
    "rfc3987",
    "parse_url",
    "parser"
  ],
  "license": "MIT",
  "homepage": "https://github.com/linkuha/php-uri",
  "authors": [
    {
      "name" : "Pudich Alexandr",
      "email" : "linkuha@gmail.com",
      "homepage" : "https://pudich.ru"
    }
  ],
  "require": {
    "php" : ">=5.6.0",
    "lib-pcre": "*"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {
      "SimpleLibs\\Uri\\": "src"
